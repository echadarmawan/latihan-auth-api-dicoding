{
  "extends": "airbnb-base",
  "env": {
    "commonjs": true,
    "es2021": true,
    "node": true,
    "jest": true
  },
  "parserOptions": {
    "ecmaVersion": 2020
  },
  "rules": {
    "linebreak-style": "off",
    "no-console": "off",
    "no-underscore-dangle": "off",
    "class-methods-use-this": "off"
  }
}
